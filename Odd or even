#include<stdio.h>    

int main()

{

  

int a[10];

int i;

printf("enter the elements in array");

for (i=0;i<10;i++)

{

scanf("%d",&a[i]);

}

printf("Even nos:\n");

for(i=0;i<10;i++)

{

    if(a[i]%2==0)

{

    printf("%d\t",a[i]);

}

}

printf("\n Odd nos:\n");

for(i=0;i<10;i++)

{

    if(a[i]%2!=0)

{

    printf("%d \t",a[i]); 

}

}

}
